#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(){
  int matquad [3][3];
  int mattrig [3][3];
  int matcirc [3][3];
  int i,j,q,t,c;
    for(i=0;i<3;i++)
        for(j=0;j<3;j++){
                matquad[0][j]=1;
                matquad[2][j]=1;
                matquad[i][0]=1;
                matquad[i][2]=1;
                
        }
    for(i=0;i<3;i++){
        for(j=0;j<3;j++){
            printf("%d",matquad[i][j]);
            printf("\t");
        }
    }
    return 0;
}
